{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Fleet Analyzer - Port Berth Round Trip",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "args": [
                "--fleet-id", "104002",
                "--start-date", "2025-06-17",
                "--end-date", "2025-06-17",
                "--output-dir", "./output/port_berth_analysis",
                "--shift-hours", "12",
                "--target-trips", "4",
                "--start-waypoint", "Port Berth_PHEVG250606",
                "--end-waypoint", "Port Berth_PHEVG250606",
                "--waypoint-matching", "exact",
                "--log-level", "INFO"
            ],
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Fleet Analyzer - Port Berth Round Trip (Debug)",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "args": [
                "--fleet-id", "104002",
                "--start-date", "2025-06-17",
                "--end-date", "2025-06-17",
                "--output-dir", "./output/port_berth_debug",
                "--shift-hours", "12",
                "--target-trips", "4",
                "--start-waypoint", "Port Berth_PHEVG250606",
                "--end-waypoint", "Port Berth_PHEVG250606",
                "--waypoint-matching", "exact",
                "--log-level", "DEBUG"
            ],
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Fleet Analyzer - All Waypoints",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "args": [
                "--fleet-id", "104002",
                "--start-date", "2025-06-17",
                "--end-date", "2025-06-17",
                "--output-dir", "./output/all_waypoints",
                "--shift-hours", "12",
                "--target-trips", "4",
                "--log-level", "INFO"
            ],
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Python: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "cwd": "${workspaceFolder}"
        }
    ]
